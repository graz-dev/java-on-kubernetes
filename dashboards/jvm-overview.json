{
  "annotations": { "list": [] },
  "description": "JVM metrics from OpenTelemetry Java agent for benchmarking",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 200,
      "title": "JVM Runtime Info",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "left",
            "displayMode": "auto",
            "minWidth": 120
          },
          "noValue": "N/A"
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Max Heap" },
            "properties": [{ "id": "unit", "value": "bytes" }]
          },
          {
            "matcher": { "id": "byName", "options": "Init Heap" },
            "properties": [{ "id": "unit", "value": "bytes" }]
          },
          {
            "matcher": { "id": "byName", "options": "G1 Region" },
            "properties": [{ "id": "unit", "value": "bytes" }]
          }
        ]
      },
      "gridPos": { "h": 4, "w": 24, "x": 0, "y": 1 },
      "id": 201,
      "options": {
        "footer": { "show": false },
        "showHeader": true,
        "sortBy": [{ "desc": false, "displayName": "Service" }]
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "group by (host_name, process_runtime_version, jvm_config_gc, jvm_config_heap_max, jvm_config_heap_init, jvm_config_gc_max_pause_ms, jvm_config_gc_threads_parallel, jvm_config_gc_threads_concurrent, jvm_config_gc_g1_region_bytes, jvm_config_jit_tiered, jvm_config_jit_compilers, jvm_config_compressed_oops, jvm_config_cpu_count) (last_over_time(jvm_memory_used_bytes{host_name=~\"$pod\"}[5m]))",
          "instant": true,
          "legendFormat": "{{host_name}}"
        }
      ],
      "title": "JVM Configuration (from -XX:+PrintFlagsFinal)",
      "transformations": [
        {
          "id": "labelsToFields",
          "options": { "mode": "columns" }
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Time": true, "Value": true },
            "renameByName": {
              "host_name": "Service",
              "process_runtime_version": "Java Version",
              "jvm_config_gc": "GC",
              "jvm_config_heap_max": "Max Heap",
              "jvm_config_heap_init": "Init Heap",
              "jvm_config_gc_max_pause_ms": "Max GC Pause (ms)",
              "jvm_config_gc_threads_parallel": "Par Threads",
              "jvm_config_gc_threads_concurrent": "Conc Threads",
              "jvm_config_gc_g1_region_bytes": "G1 Region",
              "jvm_config_jit_tiered": "Tiered JIT",
              "jvm_config_jit_compilers": "JIT Compilers",
              "jvm_config_compressed_oops": "Compressed OOPs",
              "jvm_config_cpu_count": "CPUs"
            },
            "indexByName": {
              "Service": 0,
              "Java Version": 1,
              "GC": 2,
              "Max Heap": 3,
              "Init Heap": 4,
              "Max GC Pause (ms)": 5,
              "Par Threads": 6,
              "Conc Threads": 7,
              "G1 Region": 8,
              "Tiered JIT": 9,
              "JIT Compilers": 10,
              "Compressed OOPs": 11,
              "CPUs": 12
            }
          }
        }
      ],
      "type": "table"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "id": 100,
      "title": "Heap Memory",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "fillOpacity": 20, "lineWidth": 2, "stacking": { "mode": "none" } },
          "unit": "bytes"
        },
        "overrides": [
          {
            "matcher": { "id": "byRegexp", "options": "/limit/" },
            "properties": [
              { "id": "custom.lineStyle", "value": { "dash": [10, 10], "fill": "dash" } },
              { "id": "custom.fillOpacity", "value": 0 },
              { "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }
            ]
          },
          {
            "matcher": { "id": "byRegexp", "options": "/committed/" },
            "properties": [
              { "id": "custom.lineStyle", "value": { "dash": [5, 5], "fill": "dash" } },
              { "id": "custom.fillOpacity", "value": 0 },
              { "id": "color", "value": { "fixedColor": "orange", "mode": "fixed" } }
            ]
          }
        ]
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "id": 1,
      "options": { "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] } },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "sum by (host_name) (jvm_memory_used_bytes{jvm_memory_type=\"heap\", host_name=~\"$pod\"})",
          "legendFormat": "used - {{host_name}}"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "sum by (host_name) (jvm_memory_committed_bytes{jvm_memory_type=\"heap\", host_name=~\"$pod\"})",
          "legendFormat": "committed - {{host_name}}"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "sum by (host_name) (jvm_memory_limit_bytes{jvm_memory_type=\"heap\", host_name=~\"$pod\"})",
          "legendFormat": "limit - {{host_name}}"
        }
      ],
      "title": "Heap Memory (Used / Committed / Limit)",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "fillOpacity": 30, "lineWidth": 1, "stacking": { "mode": "normal" } },
          "unit": "bytes"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
      "id": 2,
      "options": { "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] } },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "jvm_memory_used_bytes{jvm_memory_type=\"heap\", host_name=~\"$pod\"}",
          "legendFormat": "{{jvm_memory_pool_name}} - {{host_name}}"
        }
      ],
      "title": "Heap Memory by Pool",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 },
      "id": 101,
      "title": "Garbage Collection",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "bars", "fillOpacity": 80, "lineWidth": 0, "stacking": { "mode": "none" } },
          "unit": "ops"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 15 },
      "id": 3,
      "options": { "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] } },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "rate(jvm_gc_duration_seconds_count{host_name=~\"$pod\"}[2m])",
          "legendFormat": "{{jvm_gc_name}} - {{host_name}}"
        }
      ],
      "title": "GC Frequency (events/sec)",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "fillOpacity": 10, "lineWidth": 2, "stacking": { "mode": "none" } },
          "unit": "s"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 15 },
      "id": 4,
      "options": { "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] } },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "rate(jvm_gc_duration_seconds_sum{host_name=~\"$pod\"}[2m]) / rate(jvm_gc_duration_seconds_count{host_name=~\"$pod\"}[2m])",
          "legendFormat": "avg {{jvm_gc_name}} - {{host_name}}"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "histogram_quantile(0.99, rate(jvm_gc_duration_seconds_bucket{host_name=~\"$pod\"}[5m]))",
          "legendFormat": "p99 {{jvm_gc_name}} - {{host_name}}"
        }
      ],
      "title": "GC Pause Duration",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 23 },
      "id": 102,
      "title": "Threads & CPU",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "fillOpacity": 10, "lineWidth": 2, "stacking": { "mode": "none" } },
          "unit": "short"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 },
      "id": 5,
      "options": { "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] } },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "jvm_thread_count{host_name=~\"$pod\"}",
          "legendFormat": "threads (daemon={{jvm_thread_daemon}}) - {{host_name}}"
        }
      ],
      "title": "Thread Count",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "fillOpacity": 20, "lineWidth": 2, "stacking": { "mode": "none" } },
          "unit": "percentunit",
          "max": 1
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 },
      "id": 6,
      "options": { "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] } },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "jvm_cpu_recent_utilization_ratio{host_name=~\"$pod\"}",
          "legendFormat": "JVM CPU - {{host_name}}"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "rate(jvm_cpu_time_seconds_total{host_name=~\"$pod\"}[5m])",
          "legendFormat": "CPU time rate - {{host_name}}"
        }
      ],
      "title": "CPU Usage",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 32 },
      "id": 103,
      "title": "Non-Heap Memory & Classes",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "fillOpacity": 30, "lineWidth": 1, "stacking": { "mode": "normal" } },
          "unit": "bytes"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 33 },
      "id": 7,
      "options": { "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] } },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "jvm_memory_used_bytes{jvm_memory_type=\"non_heap\", host_name=~\"$pod\"}",
          "legendFormat": "{{jvm_memory_pool_name}} - {{host_name}}"
        }
      ],
      "title": "Non-Heap Memory (Metaspace, Code Cache, etc.)",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "fillOpacity": 10, "lineWidth": 2, "stacking": { "mode": "none" } },
          "unit": "short"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 33 },
      "id": 8,
      "options": { "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] } },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "jvm_class_count{host_name=~\"$pod\"}",
          "legendFormat": "loaded classes - {{host_name}}"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "rate(jvm_class_loaded_total{host_name=~\"$pod\"}[5m])",
          "legendFormat": "class load rate - {{host_name}}"
        }
      ],
      "title": "Loaded Classes",
      "type": "timeseries"
    }
  ],
  "schemaVersion": 39,
  "tags": ["jvm", "benchmarking"],
  "templating": {
    "list": [
      {
        "current": {},
        "hide": 0,
        "includeAll": false,
        "name": "datasource",
        "options": [],
        "query": "prometheus",
        "refresh": 1,
        "type": "datasource"
      },
      {
        "current": {},
        "datasource": { "type": "prometheus", "uid": "${datasource}" },
        "definition": "label_values(jvm_memory_used_bytes, host_name)",
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "name": "pod",
        "query": "label_values(jvm_memory_used_bytes, host_name)",
        "refresh": 2,
        "type": "query"
      }
    ]
  },
  "time": { "from": "now-1h", "to": "now" },
  "title": "JVM Overview",
  "uid": "jvm-overview"
}
