{
  "annotations": { "list": [] },
  "description": "JVM metrics from OpenTelemetry Java agent for benchmarking",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 100,
      "title": "Heap Memory",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "fillOpacity": 20, "lineWidth": 2, "stacking": { "mode": "none" } },
          "unit": "bytes"
        },
        "overrides": [
          {
            "matcher": { "id": "byRegexp", "options": "/limit/" },
            "properties": [
              { "id": "custom.lineStyle", "value": { "dash": [10, 10], "fill": "dash" } },
              { "id": "custom.fillOpacity", "value": 0 },
              { "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }
            ]
          },
          {
            "matcher": { "id": "byRegexp", "options": "/committed/" },
            "properties": [
              { "id": "custom.lineStyle", "value": { "dash": [5, 5], "fill": "dash" } },
              { "id": "custom.fillOpacity", "value": 0 },
              { "id": "color", "value": { "fixedColor": "orange", "mode": "fixed" } }
            ]
          }
        ]
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 1 },
      "id": 1,
      "options": { "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] } },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "sum by (host_name) (jvm_memory_used_bytes{jvm_memory_type=\"heap\", host_name=~\"$pod\"})",
          "legendFormat": "used - {{host_name}}"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "sum by (host_name) (jvm_memory_committed_bytes{jvm_memory_type=\"heap\", host_name=~\"$pod\"})",
          "legendFormat": "committed - {{host_name}}"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "sum by (host_name) (jvm_memory_limit_bytes{jvm_memory_type=\"heap\", host_name=~\"$pod\"})",
          "legendFormat": "limit - {{host_name}}"
        }
      ],
      "title": "Heap Memory (Used / Committed / Limit)",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "fillOpacity": 30, "lineWidth": 1, "stacking": { "mode": "normal" } },
          "unit": "bytes"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 1 },
      "id": 2,
      "options": { "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] } },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "jvm_memory_used_bytes{jvm_memory_type=\"heap\", host_name=~\"$pod\"}",
          "legendFormat": "{{jvm_memory_pool_name}} - {{host_name}}"
        }
      ],
      "title": "Heap Memory by Pool",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 9 },
      "id": 101,
      "title": "Garbage Collection",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "bars", "fillOpacity": 80, "lineWidth": 0, "stacking": { "mode": "none" } },
          "unit": "ops"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 10 },
      "id": 3,
      "options": { "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] } },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "rate(jvm_gc_duration_seconds_count{host_name=~\"$pod\"}[2m])",
          "legendFormat": "{{jvm_gc_name}} - {{host_name}}"
        }
      ],
      "title": "GC Frequency (events/sec)",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "fillOpacity": 10, "lineWidth": 2, "stacking": { "mode": "none" } },
          "unit": "s"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 10 },
      "id": 4,
      "options": { "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] } },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "rate(jvm_gc_duration_seconds_sum{host_name=~\"$pod\"}[2m]) / rate(jvm_gc_duration_seconds_count{host_name=~\"$pod\"}[2m])",
          "legendFormat": "avg {{jvm_gc_name}} - {{host_name}}"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "histogram_quantile(0.99, rate(jvm_gc_duration_seconds_bucket{host_name=~\"$pod\"}[5m]))",
          "legendFormat": "p99 {{jvm_gc_name}} - {{host_name}}"
        }
      ],
      "title": "GC Pause Duration",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 18 },
      "id": 102,
      "title": "Threads & CPU",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "fillOpacity": 10, "lineWidth": 2, "stacking": { "mode": "none" } },
          "unit": "short"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 19 },
      "id": 5,
      "options": { "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] } },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "jvm_thread_count{host_name=~\"$pod\"}",
          "legendFormat": "threads (daemon={{jvm_thread_daemon}}) - {{host_name}}"
        }
      ],
      "title": "Thread Count",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "fillOpacity": 20, "lineWidth": 2, "stacking": { "mode": "none" } },
          "unit": "percentunit",
          "max": 1
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 19 },
      "id": 6,
      "options": { "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] } },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "jvm_cpu_recent_utilization_ratio{host_name=~\"$pod\"}",
          "legendFormat": "JVM CPU - {{host_name}}"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "rate(jvm_cpu_time_seconds_total{host_name=~\"$pod\"}[5m])",
          "legendFormat": "CPU time rate - {{host_name}}"
        }
      ],
      "title": "CPU Usage",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 27 },
      "id": 103,
      "title": "Non-Heap Memory & Classes",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "fillOpacity": 30, "lineWidth": 1, "stacking": { "mode": "normal" } },
          "unit": "bytes"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 28 },
      "id": 7,
      "options": { "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] } },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "jvm_memory_used_bytes{jvm_memory_type=\"non_heap\", host_name=~\"$pod\"}",
          "legendFormat": "{{jvm_memory_pool_name}} - {{host_name}}"
        }
      ],
      "title": "Non-Heap Memory (Metaspace, Code Cache, etc.)",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "fillOpacity": 10, "lineWidth": 2, "stacking": { "mode": "none" } },
          "unit": "short"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 28 },
      "id": 8,
      "options": { "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] } },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "jvm_class_count{host_name=~\"$pod\"}",
          "legendFormat": "loaded classes - {{host_name}}"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "rate(jvm_class_loaded_total{host_name=~\"$pod\"}[5m])",
          "legendFormat": "class load rate - {{host_name}}"
        }
      ],
      "title": "Loaded Classes",
      "type": "timeseries"
    }
  ],
  "schemaVersion": 39,
  "tags": ["jvm", "benchmarking"],
  "templating": {
    "list": [
      {
        "current": {},
        "hide": 0,
        "includeAll": false,
        "name": "datasource",
        "options": [],
        "query": "prometheus",
        "refresh": 1,
        "type": "datasource"
      },
      {
        "current": {},
        "datasource": { "type": "prometheus", "uid": "${datasource}" },
        "definition": "label_values(jvm_memory_used_bytes, host_name)",
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "name": "pod",
        "query": "label_values(jvm_memory_used_bytes, host_name)",
        "refresh": 2,
        "type": "query"
      }
    ]
  },
  "time": { "from": "now-1h", "to": "now" },
  "title": "JVM Overview",
  "uid": "jvm-overview"
}
